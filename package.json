{
  "name": "ironhook",
  "version": "0.7.0",
  "description": "A JavaScript library for reactive programming using React-like Hooks.",
  "keywords": [
    "async",
    "asynchronous",
    "hook",
    "hooks",
    "observable",
    "observer",
    "publish",
    "pubsub",
    "react",
    "reactive",
    "subject",
    "subscribe"
  ],
  "bugs": {
    "url": "https://github.com/clebert/ironhook/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clebert/ironhook.git"
  },
  "license": "MIT",
  "author": "Clemens Akens <clebert@me.com>",
  "files": [
    "lib"
  ],
  "sideEffects": false,
  "main": "lib/cjs/index.js",
  "module": "lib/esm/index.js",
  "types": "lib/cjs/index.d.ts",
  "scripts": {
    "compile": "tsc --build tsconfig.cjs.json tsconfig.esm.json",
    "format": "prettier --write '**/*.{html,js,json,md,ts,tsx,yml}'",
    "format:test": "prettier --list-different '**/*.{html,js,json,md,ts,tsx,yml}'",
    "release": "npm version",
    "postrelease": "git push --follow-tags",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.0",
    "@babel/plugin-proposal-optional-chaining": "^7.8.0",
    "@babel/preset-env": "^7.8.2",
    "@babel/preset-typescript": "^7.8.0",
    "@size-limit/preset-small-lib": "^4.0.0",
    "@types/jest": "^25.0.0",
    "jest": "^25.0.0",
    "prettier": "^2.0.0",
    "size-limit": "^4.0.0",
    "typescript": "^3.7.4"
  },
  "size-limit": [
    {
      "path": "lib/esm/index.js",
      "limit": "1.3 KB"
    }
  ]
}
