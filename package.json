{
  "name": "ironhook",
  "version": "0.2.0",
  "description": "A JavaScript library for reactive programming using React-like Hooks.",
  "keywords": [
    "async",
    "asynchronous",
    "hook",
    "hooks",
    "react",
    "reactive"
  ],
  "bugs": {
    "url": "https://github.com/clebert/ironhook/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clebert/ironhook.git"
  },
  "license": "MIT",
  "author": "Clemens Akens <clebert@me.com>",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "precompile": "rm -rf lib",
    "compile": "tsc",
    "format": "prettier --write '**/*.{html,js,json,md,ts,tsx,yml}'",
    "release": "npm version",
    "postrelease": "git push --follow-tags",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.0",
    "@babel/plugin-proposal-optional-chaining": "^7.8.0",
    "@babel/preset-env": "^7.8.2",
    "@babel/preset-typescript": "^7.8.0",
    "@size-limit/preset-small-lib": "^3.0.0",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.6",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "typescript": "^3.7.4"
  },
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "1.5 KB"
    }
  ]
}
